<form method="post" action="{{ path('app_car_delete', {'id': car.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette voiture ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ car.id) }}">
    <button class="btn btn-danger my-2">Supprimer</button>
</form>